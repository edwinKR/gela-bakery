{
  "name": "gela-bakery-backend-server",
  "version": "1.0.0",
  "engines": {
    "node": "v12.7.0",
    "npm": "6.10.0"
  },
  "scripts": {
    "client": "cd frontend-app && npm run start",
    "server": "nodemon server.js",
    "build": "cd frontend-app && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "heroku-postbuild": "cd frontend-app && npm install && npm install --only=dev --no-shrinkwrap && npm rebuild node-sass && npm run build"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "7.0.0",
    "express": "^4.18.2",
    "stripe": "6.28.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^1.19.1"
  }
}
